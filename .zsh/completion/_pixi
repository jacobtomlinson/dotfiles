#compdef pixi

autoload -U is-at-least

_pixi() {
    typeset -A opt_args
    typeset -a _arguments_options
    local ret=1

    if is-at-least 5.2; then
        _arguments_options=(-s -S -C)
    else
        _arguments_options=(-s -C)
    fi

    local context curcontext="$curcontext" state line
    _arguments "${_arguments_options[@]}" : \
'--color=[Whether the log needs to be colored]:COLOR:(always never auto)' \
'-h[Display help information]' \
'--help[Display help information]' \
'*-v[Increase logging verbosity (-v for warnings, -vv for info, -vvv for debug, -vvvv for trace)]' \
'*--verbose[Increase logging verbosity (-v for warnings, -vv for info, -vvv for debug, -vvvv for trace)]' \
'*-q[Decrease logging verbosity (quiet mode)]' \
'*--quiet[Decrease logging verbosity (quiet mode)]' \
'--no-progress[Hide all progress bars, always turned on if stderr is not a terminal]' \
'--list[List all installed commands (built-in and extensions)]' \
'-V[Print version]' \
'--version[Print version]' \
":: :_pixi_commands" \
"*::: :->pixi" \
&& ret=0
    case $state in
    (pixi)
        words=($line[1] "${words[@]}")
        (( CURRENT += 1 ))
        curcontext="${curcontext%:*:*}:pixi-command-$line[1]:"
        case $line[1] in
            (add)
_arguments "${_arguments_options[@]}" : \
'--manifest-path=[The path to \`pixi.toml\`, \`pyproject.toml\`, or the workspace directory]:MANIFEST_PATH:_files' \
'*-p+[The platform for which the dependency should be modified]:PLATFORM:_default' \
'*--platform=[The platform for which the dependency should be modified]:PLATFORM:_default' \
'-f+[The feature for which the dependency should be modified]:FEATURE:_default' \
'--feature=[The feature for which the dependency should be modified]:FEATURE:_default' \
'-g+[The git url to use when adding a git dependency]:GIT:_default' \
'--git=[The git url to use when adding a git dependency]:GIT:_default' \
'(--tag --rev)--branch=[The git branch]:BRANCH:_default' \
'(--branch --rev)--tag=[The git tag]:TAG:_default' \
'(--branch --tag)--rev=[The git revision]:REV:_default' \
'-s+[The subdirectory of the git repository to use]:SUBDIR:_default' \
'--subdir=[The subdirectory of the git repository to use]:SUBDIR:_default' \
'--auth-file=[Path to the file containing the authentication token]:AUTH_FILE:_files' \
'--concurrent-downloads=[Max concurrent network requests, default is \`50\`]:CONCURRENT_DOWNLOADS:_default' \
'--concurrent-solves=[Max concurrent solves, default is the number of CPUs]:CONCURRENT_SOLVES:_default' \
'--pinning-strategy=[Set pinning strategy]:PINNING_STRATEGY:((semver\:"Default semver strategy e.g. \`1.2.3\` becomes \`>=1.2.3, <2\` but \`0.1.0\` becomes \`>=0.1.0, <0.2\`"
minor\:"Pin the latest minor e.g. \`1.2.3\` becomes \`>=1.2.3, <1.3\`"
major\:"Pin the latest major e.g. \`1.2.3\` becomes \`>=1.2.3, <2\`"
latest-up\:"Pin to the latest version or higher. e.g. \`1.2.3\` becomes \`>=1.2.3\`"
exact-version\:"Pin the version chosen by the solver. e.g. \`1.2.3\` becomes \`==1.2.3\`"
no-pin\:"No pinning, keep the requirement empty. e.g. \`1.2.3\` becomes \`*\`"))' \
'--pypi-keyring-provider=[Specifies whether to use the keyring to look up credentials for PyPI]:PYPI_KEYRING_PROVIDER:(disabled subprocess)' \
'--color=[Whether the log needs to be colored]:COLOR:(always never auto)' \
'(--build --pypi)--host[The specified dependencies are host dependencies. Conflicts with \`build\` and \`pypi\`]' \
'(--host --pypi)--build[The specified dependencies are build dependencies. Conflicts with \`host\` and \`pypi\`]' \
'(--host --build)--pypi[The specified dependencies are pypi dependencies. Conflicts with \`host\` and \`build\`]' \
'--no-install[Don'\''t modify the environment, only modify the lock-file]' \
'--no-lockfile-update[DEPRECATED\: use \`--frozen\` \`--no-install\`. Skips lock-file updates]' \
'--frozen[Install the environment as defined in the lockfile, doesn'\''t update lockfile if it isn'\''t up-to-date with the manifest file]' \
'--locked[Check if lockfile is up-to-date before installing the environment, aborts when lockfile isn'\''t up-to-date with the manifest file]' \
'--run-post-link-scripts[Run post-link scripts (insecure)]' \
'--tls-no-verify[Do not verify the TLS certificate of the server]' \
'--use-environment-activation-cache[Use environment activation cache (experimental)]' \
'--editable[Whether the pypi requirement should be editable]' \
'-h[Display help information]' \
'--help[Display help information]' \
'*-v[Increase logging verbosity (-v for warnings, -vv for info, -vvv for debug, -vvvv for trace)]' \
'*--verbose[Increase logging verbosity (-v for warnings, -vv for info, -vvv for debug, -vvvv for trace)]' \
'*-q[Decrease logging verbosity (quiet mode)]' \
'*--quiet[Decrease logging verbosity (quiet mode)]' \
'--no-progress[Hide all progress bars, always turned on if stderr is not a terminal]' \
'*::specs -- The dependency as names, conda MatchSpecs or PyPi requirements:_default' \
&& ret=0
;;
(a)
_arguments "${_arguments_options[@]}" : \
'--manifest-path=[The path to \`pixi.toml\`, \`pyproject.toml\`, or the workspace directory]:MANIFEST_PATH:_files' \
'*-p+[The platform for which the dependency should be modified]:PLATFORM:_default' \
'*--platform=[The platform for which the dependency should be modified]:PLATFORM:_default' \
'-f+[The feature for which the dependency should be modified]:FEATURE:_default' \
'--feature=[The feature for which the dependency should be modified]:FEATURE:_default' \
'-g+[The git url to use when adding a git dependency]:GIT:_default' \
'--git=[The git url to use when adding a git dependency]:GIT:_default' \
'(--tag --rev)--branch=[The git branch]:BRANCH:_default' \
'(--branch --rev)--tag=[The git tag]:TAG:_default' \
'(--branch --tag)--rev=[The git revision]:REV:_default' \
'-s+[The subdirectory of the git repository to use]:SUBDIR:_default' \
'--subdir=[The subdirectory of the git repository to use]:SUBDIR:_default' \
'--auth-file=[Path to the file containing the authentication token]:AUTH_FILE:_files' \
'--concurrent-downloads=[Max concurrent network requests, default is \`50\`]:CONCURRENT_DOWNLOADS:_default' \
'--concurrent-solves=[Max concurrent solves, default is the number of CPUs]:CONCURRENT_SOLVES:_default' \
'--pinning-strategy=[Set pinning strategy]:PINNING_STRATEGY:((semver\:"Default semver strategy e.g. \`1.2.3\` becomes \`>=1.2.3, <2\` but \`0.1.0\` becomes \`>=0.1.0, <0.2\`"
minor\:"Pin the latest minor e.g. \`1.2.3\` becomes \`>=1.2.3, <1.3\`"
major\:"Pin the latest major e.g. \`1.2.3\` becomes \`>=1.2.3, <2\`"
latest-up\:"Pin to the latest version or higher. e.g. \`1.2.3\` becomes \`>=1.2.3\`"
exact-version\:"Pin the version chosen by the solver. e.g. \`1.2.3\` becomes \`==1.2.3\`"
no-pin\:"No pinning, keep the requirement empty. e.g. \`1.2.3\` becomes \`*\`"))' \
'--pypi-keyring-provider=[Specifies whether to use the keyring to look up credentials for PyPI]:PYPI_KEYRING_PROVIDER:(disabled subprocess)' \
'--color=[Whether the log needs to be colored]:COLOR:(always never auto)' \
'(--build --pypi)--host[The specified dependencies are host dependencies. Conflicts with \`build\` and \`pypi\`]' \
'(--host --pypi)--build[The specified dependencies are build dependencies. Conflicts with \`host\` and \`pypi\`]' \
'(--host --build)--pypi[The specified dependencies are pypi dependencies. Conflicts with \`host\` and \`build\`]' \
'--no-install[Don'\''t modify the environment, only modify the lock-file]' \
'--no-lockfile-update[DEPRECATED\: use \`--frozen\` \`--no-install\`. Skips lock-file updates]' \
'--frozen[Install the environment as defined in the lockfile, doesn'\''t update lockfile if it isn'\''t up-to-date with the manifest file]' \
'--locked[Check if lockfile is up-to-date before installing the environment, aborts when lockfile isn'\''t up-to-date with the manifest file]' \
'--run-post-link-scripts[Run post-link scripts (insecure)]' \
'--tls-no-verify[Do not verify the TLS certificate of the server]' \
'--use-environment-activation-cache[Use environment activation cache (experimental)]' \
'--editable[Whether the pypi requirement should be editable]' \
'-h[Display help information]' \
'--help[Display help information]' \
'*-v[Increase logging verbosity (-v for warnings, -vv for info, -vvv for debug, -vvvv for trace)]' \
'*--verbose[Increase logging verbosity (-v for warnings, -vv for info, -vvv for debug, -vvvv for trace)]' \
'*-q[Decrease logging verbosity (quiet mode)]' \
'*--quiet[Decrease logging verbosity (quiet mode)]' \
'--no-progress[Hide all progress bars, always turned on if stderr is not a terminal]' \
'*::specs -- The dependency as names, conda MatchSpecs or PyPi requirements:_default' \
&& ret=0
;;
(auth)
_arguments "${_arguments_options[@]}" : \
'--color=[Whether the log needs to be colored]:COLOR:(always never auto)' \
'-h[Display help information]' \
'--help[Display help information]' \
'*-v[Increase logging verbosity (-v for warnings, -vv for info, -vvv for debug, -vvvv for trace)]' \
'*--verbose[Increase logging verbosity (-v for warnings, -vv for info, -vvv for debug, -vvvv for trace)]' \
'*-q[Decrease logging verbosity (quiet mode)]' \
'*--quiet[Decrease logging verbosity (quiet mode)]' \
'--no-progress[Hide all progress bars, always turned on if stderr is not a terminal]' \
":: :_pixi__auth_commands" \
"*::: :->auth" \
&& ret=0

    case $state in
    (auth)
        words=($line[1] "${words[@]}")
        (( CURRENT += 1 ))
        curcontext="${curcontext%:*:*}:pixi-auth-command-$line[1]:"
        case $line[1] in
            (login)
_arguments "${_arguments_options[@]}" : \
'--token=[The token to use (for authentication with prefix.dev)]:TOKEN:_default' \
'--username=[The username to use (for basic HTTP authentication)]:USERNAME:_default' \
'--password=[The password to use (for basic HTTP authentication)]:PASSWORD:_default' \
'--conda-token=[The token to use on anaconda.org / quetz authentication]:CONDA_TOKEN:_default' \
'(--token --username --password --conda-token)--s3-access-key-id=[The S3 access key ID]:S3_ACCESS_KEY_ID:_default' \
'--s3-secret-access-key=[The S3 secret access key]:S3_SECRET_ACCESS_KEY:_default' \
'--s3-session-token=[The S3 session token]:S3_SESSION_TOKEN:_default' \
'--color=[Whether the log needs to be colored]:COLOR:(always never auto)' \
'-h[Display help information]' \
'--help[Display help information]' \
'*-v[Increase logging verbosity (-v for warnings, -vv for info, -vvv for debug, -vvvv for trace)]' \
'*--verbose[Increase logging verbosity (-v for warnings, -vv for info, -vvv for debug, -vvvv for trace)]' \
'*-q[Decrease logging verbosity (quiet mode)]' \
'*--quiet[Decrease logging verbosity (quiet mode)]' \
'--no-progress[Hide all progress bars, always turned on if stderr is not a terminal]' \
':host -- The host to authenticate with (e.g. prefix.dev):_default' \
&& ret=0
;;
(logout)
_arguments "${_arguments_options[@]}" : \
'--color=[Whether the log needs to be colored]:COLOR:(always never auto)' \
'-h[Display help information]' \
'--help[Display help information]' \
'*-v[Increase logging verbosity (-v for warnings, -vv for info, -vvv for debug, -vvvv for trace)]' \
'*--verbose[Increase logging verbosity (-v for warnings, -vv for info, -vvv for debug, -vvvv for trace)]' \
'*-q[Decrease logging verbosity (quiet mode)]' \
'*--quiet[Decrease logging verbosity (quiet mode)]' \
'--no-progress[Hide all progress bars, always turned on if stderr is not a terminal]' \
':host -- The host to remove authentication for:_default' \
&& ret=0
;;
(help)
_arguments "${_arguments_options[@]}" : \
":: :_pixi__auth__help_commands" \
"*::: :->help" \
&& ret=0

    case $state in
    (help)
        words=($line[1] "${words[@]}")
        (( CURRENT += 1 ))
        curcontext="${curcontext%:*:*}:pixi-auth-help-command-$line[1]:"
        case $line[1] in
            (login)
_arguments "${_arguments_options[@]}" : \
&& ret=0
;;
(logout)
_arguments "${_arguments_options[@]}" : \
&& ret=0
;;
(help)
_arguments "${_arguments_options[@]}" : \
&& ret=0
;;
        esac
    ;;
esac
;;
        esac
    ;;
esac
;;
(build)
_arguments "${_arguments_options[@]}" : \
'--manifest-path=[The path to \`pixi.toml\`, \`pyproject.toml\`, or the workspace directory]:MANIFEST_PATH:_files' \
'--auth-file=[Path to the file containing the authentication token]:AUTH_FILE:_files' \
'--concurrent-downloads=[Max concurrent network requests, default is \`50\`]:CONCURRENT_DOWNLOADS:_default' \
'--concurrent-solves=[Max concurrent solves, default is the number of CPUs]:CONCURRENT_SOLVES:_default' \
'--pinning-strategy=[Set pinning strategy]:PINNING_STRATEGY:((semver\:"Default semver strategy e.g. \`1.2.3\` becomes \`>=1.2.3, <2\` but \`0.1.0\` becomes \`>=0.1.0, <0.2\`"
minor\:"Pin the latest minor e.g. \`1.2.3\` becomes \`>=1.2.3, <1.3\`"
major\:"Pin the latest major e.g. \`1.2.3\` becomes \`>=1.2.3, <2\`"
latest-up\:"Pin to the latest version or higher. e.g. \`1.2.3\` becomes \`>=1.2.3\`"
exact-version\:"Pin the version chosen by the solver. e.g. \`1.2.3\` becomes \`==1.2.3\`"
no-pin\:"No pinning, keep the requirement empty. e.g. \`1.2.3\` becomes \`*\`"))' \
'--pypi-keyring-provider=[Specifies whether to use the keyring to look up credentials for PyPI]:PYPI_KEYRING_PROVIDER:(disabled subprocess)' \
'-t+[The target platform to build for (defaults to the current platform)]:TARGET_PLATFORM:_default' \
'--target-platform=[The target platform to build for (defaults to the current platform)]:TARGET_PLATFORM:_default' \
'--build-platform=[The build platform to use for building (defaults to the current platform)]:BUILD_PLATFORM:_default' \
'-o+[The output directory to place the built artifacts]:OUTPUT_DIR:_files' \
'--output-dir=[The output directory to place the built artifacts]:OUTPUT_DIR:_files' \
'-b+[The directory to use for incremental builds artifacts]:BUILD_DIR:_files' \
'--build-dir=[The directory to use for incremental builds artifacts]:BUILD_DIR:_files' \
'--color=[Whether the log needs to be colored]:COLOR:(always never auto)' \
'--run-post-link-scripts[Run post-link scripts (insecure)]' \
'--tls-no-verify[Do not verify the TLS certificate of the server]' \
'--use-environment-activation-cache[Use environment activation cache (experimental)]' \
'-c[Whether to clean the build directory before building]' \
'--clean[Whether to clean the build directory before building]' \
'-h[Display help information]' \
'--help[Display help information]' \
'*-v[Increase logging verbosity (-v for warnings, -vv for info, -vvv for debug, -vvvv for trace)]' \
'*--verbose[Increase logging verbosity (-v for warnings, -vv for info, -vvv for debug, -vvvv for trace)]' \
'*-q[Decrease logging verbosity (quiet mode)]' \
'*--quiet[Decrease logging verbosity (quiet mode)]' \
'--no-progress[Hide all progress bars, always turned on if stderr is not a terminal]' \
&& ret=0
;;
(clean)
_arguments "${_arguments_options[@]}" : \
'--manifest-path=[The path to \`pixi.toml\`, \`pyproject.toml\`, or the workspace directory]:MANIFEST_PATH:_files' \
'(--build)-e+[The environment directory to remove]:ENVIRONMENT:_default' \
'(--build)--environment=[The environment directory to remove]:ENVIRONMENT:_default' \
'--color=[Whether the log needs to be colored]:COLOR:(always never auto)' \
'--activation-cache[Only remove the activation cache]' \
'--build[Only remove the pixi-build cache]' \
'-h[Display help information]' \
'--help[Display help information]' \
'*-v[Increase logging verbosity (-v for warnings, -vv for info, -vvv for debug, -vvvv for trace)]' \
'*--verbose[Increase logging verbosity (-v for warnings, -vv for info, -vvv for debug, -vvvv for trace)]' \
'*-q[Decrease logging verbosity (quiet mode)]' \
'*--quiet[Decrease logging verbosity (quiet mode)]' \
'--no-progress[Hide all progress bars, always turned on if stderr is not a terminal]' \
":: :_pixi__clean_commands" \
"*::: :->clean" \
&& ret=0

    case $state in
    (clean)
        words=($line[1] "${words[@]}")
        (( CURRENT += 1 ))
        curcontext="${curcontext%:*:*}:pixi-clean-command-$line[1]:"
        case $line[1] in
            (cache)
_arguments "${_arguments_options[@]}" : \
'--manifest-path=[The path to \`pixi.toml\`, \`pyproject.toml\`, or the workspace directory]:MANIFEST_PATH:_files' \
'--color=[Whether the log needs to be colored]:COLOR:(always never auto)' \
'--pypi[Clean only the pypi related cache]' \
'--conda[Clean only the conda related cache]' \
'--mapping[Clean only the mapping cache]' \
'--exec[Clean only \`exec\` cache]' \
'--repodata[Clean only the repodata cache]' \
'--build-backends[Clean only the build backends environments cache]' \
'--build[Clean only the build related cache]' \
'-y[Answer yes to all questions]' \
'--yes[Answer yes to all questions]' \
'-h[Display help information]' \
'--help[Display help information]' \
'*-v[Increase logging verbosity (-v for warnings, -vv for info, -vvv for debug, -vvvv for trace)]' \
'*--verbose[Increase logging verbosity (-v for warnings, -vv for info, -vvv for debug, -vvvv for trace)]' \
'*-q[Decrease logging verbosity (quiet mode)]' \
'*--quiet[Decrease logging verbosity (quiet mode)]' \
'--no-progress[Hide all progress bars, always turned on if stderr is not a terminal]' \
&& ret=0
;;
(help)
_arguments "${_arguments_options[@]}" : \
":: :_pixi__clean__help_commands" \
"*::: :->help" \
&& ret=0

    case $state in
    (help)
        words=($line[1] "${words[@]}")
        (( CURRENT += 1 ))
        curcontext="${curcontext%:*:*}:pixi-clean-help-command-$line[1]:"
        case $line[1] in
            (cache)
_arguments "${_arguments_options[@]}" : \
&& ret=0
;;
(help)
_arguments "${_arguments_options[@]}" : \
&& ret=0
;;
        esac
    ;;
esac
;;
        esac
    ;;
esac
;;
(completion)
_arguments "${_arguments_options[@]}" : \
'-s+[The shell to generate a completion script for]:SHELL:((bash\:"Bourne Again SHell (bash)"
elvish\:"Elvish shell"
fish\:"Friendly Interactive SHell (fish)"
nushell\:"Nushell"
powershell\:"PowerShell"
zsh\:"Z SHell (zsh)"))' \
'--shell=[The shell to generate a completion script for]:SHELL:((bash\:"Bourne Again SHell (bash)"
elvish\:"Elvish shell"
fish\:"Friendly Interactive SHell (fish)"
nushell\:"Nushell"
powershell\:"PowerShell"
zsh\:"Z SHell (zsh)"))' \
'--color=[Whether the log needs to be colored]:COLOR:(always never auto)' \
'-h[Display help information]' \
'--help[Display help information]' \
'*-v[Increase logging verbosity (-v for warnings, -vv for info, -vvv for debug, -vvvv for trace)]' \
'*--verbose[Increase logging verbosity (-v for warnings, -vv for info, -vvv for debug, -vvvv for trace)]' \
'*-q[Decrease logging verbosity (quiet mode)]' \
'*--quiet[Decrease logging verbosity (quiet mode)]' \
'--no-progress[Hide all progress bars, always turned on if stderr is not a terminal]' \
&& ret=0
;;
(config)
_arguments "${_arguments_options[@]}" : \
'--color=[Whether the log needs to be colored]:COLOR:(always never auto)' \
'-h[Display help information]' \
'--help[Display help information]' \
'*-v[Increase logging verbosity (-v for warnings, -vv for info, -vvv for debug, -vvvv for trace)]' \
'*--verbose[Increase logging verbosity (-v for warnings, -vv for info, -vvv for debug, -vvvv for trace)]' \
'*-q[Decrease logging verbosity (quiet mode)]' \
'*--quiet[Decrease logging verbosity (quiet mode)]' \
'--no-progress[Hide all progress bars, always turned on if stderr is not a terminal]' \
":: :_pixi__config_commands" \
"*::: :->config" \
&& ret=0

    case $state in
    (config)
        words=($line[1] "${words[@]}")
        (( CURRENT += 1 ))
        curcontext="${curcontext%:*:*}:pixi-config-command-$line[1]:"
        case $line[1] in
            (edit)
_arguments "${_arguments_options[@]}" : \
'--manifest-path=[The path to \`pixi.toml\`, \`pyproject.toml\`, or the workspace directory]:MANIFEST_PATH:_files' \
'--color=[Whether the log needs to be colored]:COLOR:(always never auto)' \
'(-g --global -s --system)-l[Operation on project-local configuration]' \
'(-g --global -s --system)--local[Operation on project-local configuration]' \
'(-l --local -s --system)-g[Operation on global configuration]' \
'(-l --local -s --system)--global[Operation on global configuration]' \
'(-l --local -g --global)-s[Operation on system configuration]' \
'(-l --local -g --global)--system[Operation on system configuration]' \
'-h[Display help information]' \
'--help[Display help information]' \
'*-v[Increase logging verbosity (-v for warnings, -vv for info, -vvv for debug, -vvvv for trace)]' \
'*--verbose[Increase logging verbosity (-v for warnings, -vv for info, -vvv for debug, -vvvv for trace)]' \
'*-q[Decrease logging verbosity (quiet mode)]' \
'*--quiet[Decrease logging verbosity (quiet mode)]' \
'--no-progress[Hide all progress bars, always turned on if stderr is not a terminal]' \
'::editor -- The editor to use, defaults to `EDITOR` environment variable or `nano` on Unix and `notepad` on Windows:_default' \
&& ret=0
;;
(list)
_arguments "${_arguments_options[@]}" : \
'--manifest-path=[The path to \`pixi.toml\`, \`pyproject.toml\`, or the workspace directory]:MANIFEST_PATH:_files' \
'--color=[Whether the log needs to be colored]:COLOR:(always never auto)' \
'--json[Output in JSON format]' \
'(-g --global -s --system)-l[Operation on project-local configuration]' \
'(-g --global -s --system)--local[Operation on project-local configuration]' \
'(-l --local -s --system)-g[Operation on global configuration]' \
'(-l --local -s --system)--global[Operation on global configuration]' \
'(-l --local -g --global)-s[Operation on system configuration]' \
'(-l --local -g --global)--system[Operation on system configuration]' \
'-h[Display help information]' \
'--help[Display help information]' \
'*-v[Increase logging verbosity (-v for warnings, -vv for info, -vvv for debug, -vvvv for trace)]' \
'*--verbose[Increase logging verbosity (-v for warnings, -vv for info, -vvv for debug, -vvvv for trace)]' \
'*-q[Decrease logging verbosity (quiet mode)]' \
'*--quiet[Decrease logging verbosity (quiet mode)]' \
'--no-progress[Hide all progress bars, always turned on if stderr is not a terminal]' \
'::key -- Configuration key to show (all if not provided):_default' \
&& ret=0
;;
(ls)
_arguments "${_arguments_options[@]}" : \
'--manifest-path=[The path to \`pixi.toml\`, \`pyproject.toml\`, or the workspace directory]:MANIFEST_PATH:_files' \
'--color=[Whether the log needs to be colored]:COLOR:(always never auto)' \
'--json[Output in JSON format]' \
'(-g --global -s --system)-l[Operation on project-local configuration]' \
'(-g --global -s --system)--local[Operation on project-local configuration]' \
'(-l --local -s --system)-g[Operation on global configuration]' \
'(-l --local -s --system)--global[Operation on global configuration]' \
'(-l --local -g --global)-s[Operation on system configuration]' \
'(-l --local -g --global)--system[Operation on system configuration]' \
'-h[Display help information]' \
'--help[Display help information]' \
'*-v[Increase logging verbosity (-v for warnings, -vv for info, -vvv for debug, -vvvv for trace)]' \
'*--verbose[Increase logging verbosity (-v for warnings, -vv for info, -vvv for debug, -vvvv for trace)]' \
'*-q[Decrease logging verbosity (quiet mode)]' \
'*--quiet[Decrease logging verbosity (quiet mode)]' \
'--no-progress[Hide all progress bars, always turned on if stderr is not a terminal]' \
'::key -- Configuration key to show (all if not provided):_default' \
&& ret=0
;;
(prepend)
_arguments "${_arguments_options[@]}" : \
'--manifest-path=[The path to \`pixi.toml\`, \`pyproject.toml\`, or the workspace directory]:MANIFEST_PATH:_files' \
'--color=[Whether the log needs to be colored]:COLOR:(always never auto)' \
'(-g --global -s --system)-l[Operation on project-local configuration]' \
'(-g --global -s --system)--local[Operation on project-local configuration]' \
'(-l --local -s --system)-g[Operation on global configuration]' \
